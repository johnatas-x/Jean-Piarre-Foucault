{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "packageRules": [
    {
      "groupName": "Drupal core",
      "matchPackageNames": [
        "drupal/core-**"
      ]
    },
    {
      "groupName": "NPM asset",
      "matchPackageNames": [
        "npm-asset/**"
      ]
    }
  ]
}
